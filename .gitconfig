[user]
  email = jakob.leczinsky@gmail.com
  name = Jakob Leczinsky
[core]
  excludesfile = /Users/jaklec/.gitignore
	pager = diff-so-fancy | less --tabs=4 -RFX
[includeif "gitdir:~/Projects/seenthis/"]
  path = ~/.gitconfig-seenthis
[pull]
  rebase = false
[alias]
  fixup = commit --amend --no-edit
  undo = reset --soft HEAD^
[init]
	defaultBranch = main
[interactive]
	diffFilter = diff-so-fancy --patch
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[credential]
	helper = osxkeychain
